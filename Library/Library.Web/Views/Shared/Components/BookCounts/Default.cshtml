@model IEnumerable<Library.Core.DTOs.ShowGroupsViewModels>

<section class="category-section border-radius">
    <header><i class="fa fa-folder-open-o"></i> <h3> دسته بندی ها </h3></header>
    <ul>
        @foreach (var item in Model)
        {
            <li><a href="/Books/@item.GroupId/@item.GroupTiltle"> @item.GroupTiltle </a>
                @*<span>@item.GroupCount</span>*@
            </li>
        }

    </ul>
</section>